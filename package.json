{
  "name": "scratchblocks",
  "version": "3.0.0",
  "description": "Render Scratch blocks code to SVG.",
  "directories": {
    "test": "tests"
  },
  "main": "src/scratchblocks.js",
  "scripts": {
    "browserify": "SB_TARGET='client' browserify --standalone scratchblocks -t envify -t unreachable-branch-transform src/scratchblocks.js > src/scratchblocks.browser.js",
    "build": "npm run build-make",
    "build-make": "sh -c 'make all'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjvr/scratchblocks.git"
  },
  "keywords": [
    "scratch"
  ],
  "author": "tjvr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjvr/scratchblocks/issues"
  },
  "homepage": "https://scratchblocks.github.io/",
  "devDependencies": {
    "browserify": "^13.0.1",
    "clean-css": "^3.4.9",
    "envify": "^3.4.1",
    "uglify-js": "^2.6.1",
    "unreachable-branch-transform": "^0.5.1"
  },
  "dependencies": {
    "canvas": "1.4.0",
    "xmldom": "^0.1.22"
  }
}
